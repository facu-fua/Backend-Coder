{{>header}}
<div>
    <h1>Welcome to WoodenWoods!</h1>
<button>Login</button>
<button>Cart</button>
<button>Register</button>
</div>
<div class="productos">
    {{#each products}}
        <div class="producto">
            <h2>{{this.name}}</h2>
            <h3>{{this.price}}</h3>
            <p>{{this.description}}</p>
            <button>Agregar al carrito</button>
        </div>
    {{/each}}
</div>

{{>footer}}